<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joanneum.motivation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".history.History"
            android:parentActivityName= ".MainActivity" />
        <activity android:name=".reward.Reward"
            android:parentActivityName= ".MainActivity" />
        <activity android:name=".selection.CategorySelection"
            android:launchMode="singleTop"
            android:parentActivityName= ".MainActivity" />
        <activity android:name=".selection.TaskSelection"
            android:parentActivityName= ".selection.CategorySelection" />
        <activity android:name=".selection.Success" />
        <activity android:name=".quest.Quest"
            android:parentActivityName= ".MainActivity" />
        <activity android:name=".quest.Waiting"
            android:parentActivityName= ".selection.CategorySelection" />
        <activity android:name=".ranking.Ranking"
            android:parentActivityName= ".MainActivity" />
        <activity android:name=".Registration" />
        <activity android:name=".profil.Profile"
            android:parentActivityName= ".MainActivity" />


    </application>

</manifest>